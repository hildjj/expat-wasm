{
  "name": "expat-wasm",
  "version": "1.0.4",
  "description": "Evented parser based on a WASM-compiled version of expat",
  "main": "lib/index.js",
  "scripts": {
    "test": "ava test",
    "coverage": "nyc npm test",
    "postcoverage": "nyc report -r html",
    "lint": "standard",
    "doc": "jsdoc -c .jsdoc.conf",
    "prerelease": "npm run lint && npm run test && npm run doc && git diff --quiet --exit-code",
    "release": "npm version patch && git push --follow-tags && npm publish",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "engines": {
    "node": ">=9.3.0"
  },
  "keywords": [
    "xml",
    "parse",
    "expat",
    "wasm"
  ],
  "author": "Joe Hildebrand <joe-github@cursive.net>",
  "license": "MPL-2.0",
  "devDependencies": {
    "ava": "^1.2.1",
    "jsdoc": "^3.5.5",
    "minami": "^1.2.3",
    "nyc": "^13.2.0",
    "standard": "^12.0.1"
  },
  "repository": "hildjj/expat-wasm",
  "standard": {
    "ignore": [
      "lib/expat.js",
      "docs/"
    ]
  },
  "nyc": {
    "exclude": [
      "lib/expat.js",
      "test/"
    ]
  }
}

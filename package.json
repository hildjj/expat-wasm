{
  "name": "expat-wasm",
  "version": "3.1.3",
  "description": "Evented parser based on a WASM-compiled version of expat",
  "main": "lib/index.js",
  "type": "module",
  "types": "types/index.d.ts",
  "repository": "hildjj/expat-wasm",
  "scripts": {
    "test": "ava",
    "coverage": "c8 npm test",
    "lint": "eslint .",
    "doc": "jsdoc -c .jsdoc.conf",
    "ts": "tsc",
    "webpack": "cd webpack-demo && npm run build",
    "postwebpack": "copyfiles -V webpack-demo/index.html webpack-demo/dist/* docs/",
    "build": "(cd wasm && ./setup && ./wmak) && npm run ts && npm run doc && npm run webpack && npm run lint"
  },
  "keywords": [
    "xml",
    "parse",
    "expat",
    "wasm"
  ],
  "author": "Joe Hildebrand <joe-github@cursive.net>",
  "license": "MIT",
  "devDependencies": {
    "@cto.af/eslint-config": "4.0.0",
    "@types/emscripten": "1.39.10",
    "@types/node": "20.11.25",
    "ava": "^6.1.2",
    "c8": "9.1.0",
    "copyfiles": "2.4.1",
    "docdash": "2.0.2",
    "eslint": "8.57.0",
    "jsdoc": "^4.0.2",
    "typescript": "5.4.2"
  },
  "packageManager": "pnpm@8.15.4",
  "engines": {
    "node": ">=18"
  }
}
